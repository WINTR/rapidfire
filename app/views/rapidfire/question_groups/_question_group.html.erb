<tr id= "question_group_<%= question_group.id %>">
  <td>
    <%= question_group.name %>
  </td>
  <td>
    <ul class="horizontal-list">
      <li><%= link_to "Take Survey", new_question_group_answer_group_path(question_group) %></li>
      <% if can_administer? %>
        <li><%= link_to "Survey Results", [:results, question_group] %></li>
        <li><%= link_to "Edit Survey", question_group_questions_path(question_group) %></li>
        <li><%= link_to "Delete Survey", [question_group], method: :delete %></li>
      <% end %>
    </ul>
  </td>
</tr>
